
public class MergeSort {
	
	public static int Merge(int arr[],int Low,int Mid,int High)
	{
		int Size1=Mid-1;
		int Size2=High-(Mid+1);
		int Brr[]=new int[Size1];
		int Crr[]=new int[Size2];
		for(int i=0;i<Brr.length;i++)
		{
			Brr[i]=arr[Low+i];
		}
		for(int i=0;i<Crr.length;i++)
		{
			Crr[i]=arr[Mid+1+(i)];
		}
		
	}

}
